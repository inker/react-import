{
  "name": "react-import",
  "version": "1.1.0",
  "description": "Lazily import React components",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:commonjs": "rimraf lib && cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "rimraf es && cross-env BABEL_ENV=es babel src --out-dir es",
    "build": "npm run build:commonjs && npm run build:es",
    "build-publish": "sh build/publish.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/inker/react-import"
  },
  "keywords": [
    "react",
    "lazy",
    "load",
    "import",
    "component"
  ],
  "author": "Anton Veselyev",
  "license": "ISC",
  "peerDependencies": {
    "react": ">=15.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.3",
    "rimraf": "^2.6.2"
  }
}
